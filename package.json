{
  "name": "dexchange-api-sdk",
  "version": "1.0.5",
  "description": "SDK officiel pour int√©grer DEXCHANGE-API dans vos applications JavaScript/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "dev": "tsc --watch",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "docs": "typedoc --out docs src",
    "clean": "rimraf dist coverage docs"
  },
  "keywords": [
    "dexchange",
    "payment",
    "sdk",
    "typescript",
    "api",
    "mobile-money",
    "africa",
    "fintech",
    "payment-gateway"
  ],
  "author": "DEXCHANGE <team@dexchange.sn>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "axios-retry": "^3.9.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEXCHANGE-GROUP/dexchange-api-typescript-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/DEXCHANGE-GROUP/dexchange-api-typescript-js-sdk/issues"
  },
  "homepage": "https://docs-api.dexchange.sn",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "scope": "@dexchange"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
